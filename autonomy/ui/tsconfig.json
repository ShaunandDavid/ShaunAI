{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": { "@/*": ["src/*"] },
  "types": ["vite/client", "node"]
  ,"strict": true
  ,"noImplicitAny": true
  ,"noUnusedLocals": true
  ,"noUnusedParameters": true
  ,"noImplicitReturns": true
  ,"noFallthroughCasesInSwitch": true
  },
  "include": ["src"]
}
